def convert_to_python(s):
    global sl
    f = 0
    for key, value in sl.items():
        if key in s:
            print("Name '" + key + "' is not defined")
            f = 1
            break
        s = s.replace(value, key)

    if f == 0:
        exec(s)

def convert_to_python2(s):
    global sl
    f = 0
    for key, value in sl2.items():
        if key in s:
            print("Name '" + key + "' is not defined")
            f = 1
            break
        s = s.replace(value, key)

    if f == 0:
        exec(s)        

def convert_to_mylang(s):
    global sl
    f = 0
    for key, value in sl.items():
        s = s.replace(key, value)
    return s

def open_file(x):
    with open(x, 'r') as f:
        s = f.read()
    return s

x = input("Ð’Ñ‹ Ñ…Ð¾Ñ‚Ð¸Ñ‚Ðµ Ð·Ð°Ð¿ÑƒÑÑ‚Ð¸Ñ‚ÑŒ ÐºÐ¾Ð´ Ð¸Ð· txt Ñ„Ð°Ð¹Ð»Ð° Ð¸Ð»Ð¸ Ð²Ð²ÐµÑÑ‚Ð¸ ÐµÐ³Ð¾ Ð² ÐºÐ¾Ð¼Ð°Ð½Ð´Ð½ÑƒÑŽ ÑÑ‚Ñ€Ð¾ÐºÑƒ?(Ð•ÑÐ»Ð¸ Ð·Ð°Ð¿ÑƒÑÐº Ð¸Ð· Ñ„Ð°Ð¹Ð»Ð°, Ð²Ð²ÐµÐ´Ð¸Ñ‚Ðµ Ð¸Ð¼Ñ Ñ„Ð°Ð¹Ð»Ð°, Ð° ÐµÑÐ»Ð¸ Ð²Ñ‹ Ð±ÑƒÐ´ÐµÑ‚Ðµ Ð²Ð²Ð¾Ð´Ð¸Ñ‚ÑŒ ÐºÐ¾Ð´ ÑÐ°Ð¼Ð¾ÑÑ‚Ð¾ÑÑ‚ÐµÐ»ÑŒÐ½Ð¾, Ð²Ð²ÐµÐ´Ð¸Ñ‚Ðµ 'code' Ð¿ÐµÑ€ÐµÐ´ Ð¿ÐµÑ€Ð²Ð¾Ð¹ ÑÑ‚Ñ€Ð¾ÐºÐ¾Ð¹ ÐºÐ¾Ð´Ð° Ð¸ 0 Ð¿Ð¾ÑÐ»Ðµ Ð¿Ð¾ÑÐ»ÐµÐ´Ð½ÐµÐ¹)")
if x == 'code':
    m1 = []
    s1 = ''
    while s1 != '0':
        s1 = input()
        if s1 != '0':
            m1.append(s1 + '\n')
    s = ''.join(m1)
    convert_to_python(s)
else:
    s = open_file(x)
    convert_to_python2(s)
